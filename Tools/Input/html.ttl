    rdfs:range xsd:string;
    skos:definition 'Property that links the key name of a specific HTML attribute to that HTML attribute.'@en;
    skos:prefLabel 'has key name'@en.

  html:tag
    a owl:DatatypeProperty;
    dct:conformsTo
      section:13.1.2.1,
      section:13.1.2.2;
