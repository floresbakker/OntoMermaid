html:Audio a owl:Class ;
    dct:conformsTo section:4.8.9 ;
    rdfs:subClassOf html:Element ;
    skos:definition "The <audio> HTML element is used to embed sound content in documents. It may contain one or more audio sources, represented using the src attribute or the <source> element: the browser will choose the most suitable one. It can also be the destination for streamed media, using a MediaStream."@en ;
    skos:prefLabel "the audio element"@en ;
    html:tag "audio" ;
    mermaid:label """https://data.rijksfinancien.nl/html/model/def/Audio((the audio element)) -- subClassOf --> https://data.rijksfinancien.nl/html/model/def/Element
""" ;
    mermaid:syntax "SUBCLASSOF" .

html:Base a owl:Class ;
    dct:conformsTo section:4.2.3 ;
    rdfs:subClassOf html:VoidElement ;
    skos:definition "The <base> HTML element specifies the base URL to use for all relative URLs in a document. There can be only one <base> element in a document."@en ;
    skos:prefLabel "the base element"@en ;
    html:tag "base" ;
    mermaid:label """https://data.rijksfinancien.nl/html/model/def/Base((the base element)) -- subClassOf --> https://data.rijksfinancien.nl/html/model/def/VoidElement
""" ;
    mermaid:syntax "SUBCLASSOF" .

html:Canvas a owl:Class ;
    dct:conformsTo section:4.12.5 ;
    rdfs:subClassOf html:Element ;
    skos:definition "Use the HTML <canvas> element with either the canvas scripting API or the WebGL API to draw graphics and animations."@en ;
    skos:prefLabel "the canvas element"@en ;
    html:tag "canvas" ;
    mermaid:label """https://data.rijksfinancien.nl/html/model/def/Canvas((the canvas element)) -- subClassOf --> https://data.rijksfinancien.nl/html/model/def/Element
""" ;
    mermaid:syntax "SUBCLASSOF" .

html:Form a owl:Class ;
    dct:conformsTo section:4.10.3 ;
    rdfs:subClassOf html:Element ;
    skos:definition "The <form> HTML element represents a document section containing interactive controls for submitting information."@en ;
    skos:prefLabel "the form element"@en ;
    html:tag "form" ;
    mermaid:label """https://data.rijksfinancien.nl/html/model/def/Form((the form element)) -- subClassOf --> https://data.rijksfinancien.nl/html/model/def/Element
""" ;
    mermaid:syntax "SUBCLASSOF" .

html:Label a owl:Class ;
    dct:conformsTo section:4.10.4 ;
    rdfs:subClassOf html:Element ;
    skos:definition "The <label> HTML element represents a caption for an item in a user interface."@en ;
    skos:prefLabel "the label element"@en ;
    html:tag "label" ;
    mermaid:label """https://data.rijksfinancien.nl/html/model/def/Label((the label element)) -- subClassOf --> https://data.rijksfinancien.nl/html/model/def/Element
""" ;
    mermaid:syntax "SUBCLASSOF" .

html:Object a owl:Class ;
    dct:conformsTo section:4.8.7 ;
    rdfs:subClassOf html:Element ;
    skos:definition "The <object> HTML element represents an external resource, which can be treated as an image, a nested browsing context, or a resource to be handled by a plugin."@en ;
    skos:prefLabel "the object element"@en ;
    html:tag "object" ;
    mermaid:label """https://data.rijksfinancien.nl/html/model/def/Object((the object element)) -- subClassOf --> https://data.rijksfinancien.nl/html/model/def/Element
""" ;
    mermaid:syntax "SUBCLASSOF" .

html:Script a owl:Class ;
    dct:conformsTo section:4.12.1 ;
    rdfs:subClassOf html:Element ;
    skos:definition "The <script> HTML element is used to embed executable code or data; this is typically used to embed or refer to JavaScript code. The <script> element can also be used with other languages, such as WebGL's GLSL shader programming language and JSON."@en ;
    skos:prefLabel "the script element"@en ;
    html:tag "script" ;
    mermaid:label """https://data.rijksfinancien.nl/html/model/def/Script((the script element)) -- subClassOf --> https://data.rijksfinancien.nl/html/model/def/Element
""" ;
    mermaid:syntax "SUBCLASSOF" .

html:Table a owl:Class ;
    dct:conformsTo section:4.9.1 ;
    rdfs:subClassOf html:Element ;
    skos:definition "A table (<table> element) represents data with more than one dimension.  Tables have rows, columns, and cells given by their descendants.  The rows and columns form a grid; a table its cells must completely cover that grid without overlap."@en ;
    skos:prefLabel "the table element"@en ;
    html:tag "table" ;
    mermaid:label """https://data.rijksfinancien.nl/html/model/def/Table((the table element)) -- subClassOf --> https://data.rijksfinancien.nl/html/model/def/Element
""" ;
    mermaid:syntax "SUBCLASSOF" .

html:TableFooter a owl:Class ;
    dct:conformsTo section:4.9.7 ;
    rdfs:subClassOf html:Element ;
    skos:definition "The table footer (<tfoot> element) represents the block of rows that consist of the column summaries (footers) for the parent table."@en ;
    skos:prefLabel "the tfoot element"@en ;
    html:tag "tfoot" ;
    mermaid:label """https://data.rijksfinancien.nl/html/model/def/TableFooter((the tfoot element)) -- subClassOf --> https://data.rijksfinancien.nl/html/model/def/Element
""" ;
    mermaid:syntax "SUBCLASSOF" .

html:Template a owl:Class ;
    dct:conformsTo section:4.12.3 ;
    rdfs:subClassOf html:Element ;
    skos:definition "The <template> HTML element is a mechanism for holding HTML that is not to be rendered immediately when a page is loaded but may be instantiated subsequently during runtime using JavaScript."@en ;
    skos:prefLabel "the template element"@en ;
    html:tag "template" ;
    mermaid:label """https://data.rijksfinancien.nl/html/model/def/Template((the template element)) -- subClassOf --> https://data.rijksfinancien.nl/html/model/def/Element
""" ;
    mermaid:syntax "SUBCLASSOF" .

html:TextElement a owl:Class ;
    rdfs:subClassOf dom:Element ;
    skos:definition "A DOM element that has textual content and that does not contain any HTML vocabulary."@en ;
    skos:example "A natural language sentence that appears in a table caption."@en,
        "A numeric value that appears in a table cell."@en ;
    skos:prefLabel "text element"@en ;
    mermaid:label """https://data.rijksfinancien.nl/html/model/def/TextElement((text element)) -- subClassOf --> https://data.rijksfinancien.nl/dom/model/def/Element
""" ;
    mermaid:syntax "SUBCLASSOF" .

html:Track a owl:Class ;
    dct:conformsTo section:4.8.10 ;
    rdfs:subClassOf html:VoidElement ;
    skos:definition "The <track> HTML element is used as a child of the media elements, <audio> and <video>. It lets you specify timed text tracks (or time-based data), for example to automatically handle subtitles. The tracks are formatted in WebVTT format (.vtt files) - Web Video Text Tracks."@en ;
    skos:prefLabel "the track element"@en ;
    html:tag "track" ;
    mermaid:label """https://data.rijksfinancien.nl/html/model/def/Track((the track element)) -- subClassOf --> https://data.rijksfinancien.nl/html/model/def/VoidElement
""" ;
    mermaid:syntax "SUBCLASSOF" .

