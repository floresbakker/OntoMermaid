        mermaid:fragment "n00655dc89c7f4597b812adc3eb88cdb8b19 -- http://www.w3.org/ns/prov#qualifiedInfluence --> http://www.w3.org/ns/prov#Influence" ;
        mermaid:syntax "OBJECTPROPERTY" .

