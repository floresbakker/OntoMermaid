        mermaid:fragment "http://www.w3.org/ns/prov#Entity -- http://www.w3.org/ns/prov#wasGeneratedBy --> http://www.w3.org/ns/prov#Activity" ;
        mermaid:syntax "OBJECTPROPERTY" .

    prov:wasInvalidatedBy a owl:ObjectProperty ;
        rdfs:label "wasInvalidatedBy" ;
